{
  "name": "starter-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Starter Template",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "create:apk":"rm -rf android && npm run build && npx cap add android && npx cap copy android && cd android && ./gradlew assembleRelease",
    "create:debug-apk":"rm -rf android && npm run build && npx cap add android && npx cap copy android && cd android && ./gradlew assembleDebug",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor/android": "^7.4.1",
    "@capacitor/app": "^7.0.1",
    "@capacitor/browser": "^7.0.1",
    "@capacitor/core": "^7.4.1",
    "@capacitor/ios": "^7.4.1",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "dom7": "^4.0.6",
    "framework7": "^8.3.4",
    "framework7-icons": "^5.0.5",
    "framework7-svelte": "^8.3.4",
    "material-icons": "^1.13.14",
    "skeleton-elements": "^4.0.1",
    "svelte": "^4.2.0",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^24.0.10",
    "autoprefixer": "^10.4.21",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.2.4",
    "svelte-check": "^4.2.2",
    "svelte-preprocess": "^6.0.3",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.8.3",
    "vite": "^5.4.8"
  }
}
